<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileApps2Project.MatchSettingsPage">

    <ContentPage.Content>
        <StackLayout>

            <StackLayout x:Name="toolbarStk">
                <Label x:Name="bar"/>
            </StackLayout>

            <Grid x:Name="settingsGrd">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>

            </Grid>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>